    "proto:build": "nx run-many --target=buf --all --skipNxCache"
  },
  "devDependencies": {
    "@bufbuild/buf": "1.47.2",
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "19.6.0",
