{
  "name": "shulker-proxy-api",
  "root": "packages/shulker-proxy-api",
  "sourceRoot": "packages/shulker-proxy-api/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["dist/support/shulker-proxy-api"],
      "options": {
        "command": "gradle :packages:shulker-proxy-api:build -x check"
      },
      "inputs": ["default", "java:dependencies"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "gradle :packages:shulker-proxy-api:test"
      },
      "inputs": ["default", "java:dependencies"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "gradle :packages:shulker-proxy-api:ktlintCheck"
      },
      "inputs": ["default", "java:dependencies"]
    },
    "publish-maven": {
      "executor": "nx:run-commands",
      "options": {
        "command": "gradle :packages:shulker-proxy-api:publish"
      }
    }
  },
  "tags": ["lang:java"]
}
