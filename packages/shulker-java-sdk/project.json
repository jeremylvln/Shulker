{
  "name": "shulker-java-sdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/shulker-java-sdk/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/packages/shulker-java-sdk"],
      "options": {
        "command": "./gradlew :packages:shulker-java-sdk:build -x check"
      },
      "inputs": ["default", "java:dependencies"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew :packages:shulker-java-sdk:test"
      },
      "inputs": ["default", "java:dependencies"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./gradlew :packages:shulker-java-sdk:ktlintCheck"
      },
      "inputs": ["default", "java:dependencies"]
    }
  },
  "tags": ["lang:java"],
  "implicitDependencies": []
}
